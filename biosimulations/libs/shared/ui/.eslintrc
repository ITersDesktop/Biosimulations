{
  "extends": "../../../.eslintrc",
  "rules": {},
  "ignorePatterns": ["!**/*", "**/*.spec.ts", "**/table.component.html"],
  "overrides": [
    {
      "files": ["**/*.component.html"],
      "parser": "@angular-eslint/template-parser",
      "plugins": ["@angular-eslint/template"],

      "rules": {
        "@angular-eslint/template/cyclomatic-complexity": [
          "error",
          { "maxComplexity": 10 }
        ]
      }
    }
  ]
}
